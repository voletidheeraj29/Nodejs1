package Dateandtime;

import java.util.Date;

public class Date1{
public static void main(String args[]){
	Date date = new Date();
	
	 SimpleDateFormat ft = 
		      new SimpleDateFormat ("E yyyy.MM.dd 'at' hh:mm:ss a zzz");
	 System.out.println(date);
		      System.out.println("Current Date: " + ft.format(date));
}
}
